{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      },
      {
        "src": "./main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "main.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  